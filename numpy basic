# importing numpy library
import numpy as np

a=np.array([2,3,4,5])  # creating an array
b=np.array([5,6,3,7])

c=a+b    # it adds a and b and return an element
print(c)

print(a*2)  # it multiply 2 with each element of array
print(a+5)  # it add 5 in each element of array
print(b**2)  # it square each element of b
print(a*b)   # it multiply a and b  corrosponding element 
